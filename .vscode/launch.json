    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Dev Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceRoot}/node_modules/.bin/vue-cli-service",
            "windows": {
                "program": "${workspaceRoot}/node_modules/@vue/cli-service/bin/vue-cli-service.js",
            },
            "args": ["express:watch"],
            "console": "internalConsole",
            "sourceMaps": true,
            "outFiles": []
        },
    ]
}